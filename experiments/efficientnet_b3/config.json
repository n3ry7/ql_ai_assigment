  "lr_patience": 5,
  "min_lr": 1e-06,
  "gradient_clip": 1.0,
  "dropout_rate": 0.5,
  "augment": true,
  "checkpoint_interval": 5,
  "num_workers": 4,
  "pin_memory": true,
  "output_dir": "./experiments/efficientnet_b3"
}