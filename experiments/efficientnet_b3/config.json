{
  "model_type": "efficientnet_b3",
  "num_classes": 3,
  "input_channels": 1,
  "pretrained": false,
  "target_size": [
    512,
    512
  ],
  "grayscale": true,
  "maintain_aspect_ratio": true,
  "epochs": 100,
  "batch_size": 16,
